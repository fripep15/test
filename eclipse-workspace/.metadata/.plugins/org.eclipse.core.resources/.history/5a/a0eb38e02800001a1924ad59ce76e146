package replall;

public class ExerciseAdvanceLoop {
	public static void main (String [] args) {
		
	
		String [] name=new String [];
		
		name[0]= "koji";
		name[1]="kalo";
		name[2]="shalo";
		name[3]="walo";
		
		for (int i=0; i<=name.length; i++) {
		System.out.println(name[i]);	
		}
		
		
		System.out.println("**************************************************************");
		
		String[] str= {"shamim","azim","kalim","HAJI"};
	
		
		for (int i=0; i<=str.length; i++) {
			System.out.println(str[i]);
			
			//Advance Array loop
			
			
		}
		
		System.out.println("*******************************************************************");
		for (String str1:str) {
			System.out.println(str1);
		
		
	
		}
	}

}
