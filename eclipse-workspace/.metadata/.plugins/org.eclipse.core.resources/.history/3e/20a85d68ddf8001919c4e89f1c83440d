package com.class12;

public class TaskContains {

	public static void main(String[] args) {
		

		// Contains
		
		 String practice= " It was raining";
		 System.out.println(practice.contains("was"));
		
		// Subset , ask is this set included in the other string
		 

		 String Str1= " It was raining";
		 String Str2="was ";
		 System.out.println(Str1.contains(Str2));
		 
		 /*
		  * sen.length();
         * sen.equals();
         * sen.contains(s);
         * sen.toUpperCase();
         * sen.toLowerCase();
         * sen.equalsIgnoreCase(anotherString);
		  */
		 
		 System.out.println(practice.length());
	}

}
