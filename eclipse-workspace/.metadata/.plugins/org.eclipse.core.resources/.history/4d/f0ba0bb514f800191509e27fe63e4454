package com.class11;

public class TwoDimensionalArray {

	public static void main(String[] args) {
		
		// Creat 2D array with 2 rows and 2 coloumns,
		
		// Declare 2D Array
		String [] []  array=new String[2][3];
		// Row 1:
		array[0][0]="Khan";
		array[0][1]="Zaman";
		array[0][2]="Jahn";
		array[0][3]="watan";
		
		// Row 2
		
		array[1][0]="sKhan";
		array[1][1]="sZaman";
		array[1][2]="sJahn";
		array[1][3]="swatan";
		
		System.out.println(array [1][1]);
		
		
	}

}
