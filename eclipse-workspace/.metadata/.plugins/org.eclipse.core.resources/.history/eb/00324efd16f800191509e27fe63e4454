package com.class11;

public class TwoDimensionalArray {

	public static void main(String[] args) {
		
		// Create 2D array with 2 rows and 3 coloumns,
		
		// Declare 2D Array
		String [] []  array=new String[2][3];
		// Row 1:
		array[0][0]="Khan";
		array[0][1]="Zaman";
		array[0][2]="Jahn";
		
		
		// Row 2
		
		array[1][0]="sKhan";
		array[1][1]="sZaman";
		array[1][2]="sJahn";
		array[1][3]="swatan";
		
		
		
		System.out.println(array [1][1]);
	
	
int [][]numbers= {
		{2,3,4,5,6},
		{3,4,5,6,9},
		{4,5,6,7,8},
		
};

  System.out.println( numbers [1][1]);

}}
