package Day05review;

public class primitive_Casting {
	public static void main(String[] args) {
		
	

	
	// 128:
	short SNum = 128; int INum = 128;
	long LNum =128l;   float FNum=128F;
	double DNum=128; char CNum=128;
	System.out.println(SNum);
	System.out.println(INum);
	System.out.println(DNum);
	System.out.println(FNum);
	
	// 40000
  int INum2 = 40000;
	long LNum2 =40000l;   float FNum2=40000;
	double DNum2=40000; char CNum2 = 40000;
	System.out.println(INum2);
	System.out.println(LNum2);
	System.out.println(FNum2);
	System.out.println(DNum2);
	System.out.println(CNum2);
	
	System.out.println("3"+3); // 33
	System.out.println('3'+3); //54
	System.out.println("3"+'3'); //33
	System.out.println('3'+3+"3");//543
	System.out.println("2"+2+(2+2));//224
	System.out.println("2"+2+'2'+(2+2));//2224
	System.out.println(2+'2'+(2+2));//56
	System.out.println("2"+('2'+2+2));//256
	
	
	
	
	}	
}
